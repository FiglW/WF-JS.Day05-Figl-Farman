{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "title": "employee",
   "description": "employee data to sell",
   "type": "object",
	
   "properties": {
	
      "uniqueID": {
         "description": "The unique identifier for a product",
         "type": "integer"
      },
		
      "firstname": {
         "description": "Name of the product",
         "type": "string"
      },
		
      "lastname": {
         "type": "string",
      },
       "emailadress": {
         "type": "string",
      },
       "jobtitle": {
         "type": "string",
        
      },
       "salary": {
         "type": "number",
         "minimum": 200,
         "exclusiveMinimum": true
      }
   },
	
   "required": ["uniqueID", "firstname", "lastname","emailadress","jobtitle","salary"]
}


